<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vBox" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fileparser.javaFX.controller.MainController">
   <children>
      <HBox spacing="20.0">
         <children>
            <Label fx:id="firstPathLabel" prefHeight="25.0" prefWidth="300.0" styleClass="path-label" text="Please choose the first file" />
            <Button fx:id="firstFileChooserButton" mnemonicParsing="false" onAction="#chooseFirstFile" prefHeight="25.0" prefWidth="93.0" text="Open" />
         </children>
      </HBox>
      <TableView fx:id="firstTableView" prefHeight="180.0" prefWidth="566.0" />
      <HBox spacing="20.0">
         <children>
            <Label fx:id="secondPathLabel" prefHeight="25.0" prefWidth="300.0" styleClass="path-label" text="Please choose the second file" />
            <Button fx:id="secondFileChooserButton" mnemonicParsing="false" onAction="#chooseSecondFile" prefHeight="25.0" prefWidth="93.0" text="Open" />
         </children>
      </HBox>
      <TableView fx:id="secondTableView" prefHeight="180.0" prefWidth="566.0" />
      <HBox spacing="20.0">
         <children>
            <Button fx:id="executeButton" mnemonicParsing="false" onAction="#execute" prefHeight="25.0" prefWidth="102.0" text="Execute" />
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveReport" text="Save" visible="false" />
         </children>
      </HBox>
      <TableView fx:id="table" prefHeight="180.0" prefWidth="526.0" visible="false">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</VBox>
